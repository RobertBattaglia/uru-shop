{
  "extends": ["airbnb", "plugin:jest/recommended"],
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "rules": {
    "no-console": "off",
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "max-len": ["error", { "code": 1000 }]
  }
}
